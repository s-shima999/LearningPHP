<!-- ログ出力設定 -->
<SETTING>
    <!-- 出力有無(True/False) -->
    <OUTPUT>True</OUTPUT>

    <!-- 出力フォルダパス -->
    <PATH>./log</PATH>

    <!-- 出力ファイル名 -->
    <NAME>test</NAME>

    <!-- 出力ファイル拡張子 -->
    <EXTENTION>.log</EXTENTION>

    <!-- ログ出力レベル(DEBUG/INFOR/WARN/ERROR) -->
    <LEVEL>DEBUG</LEVEL>

</SETTING>